{
  "Tapeworm": {
    "Body Structure": {
      "Shape": "Elongated ribbon or tape-like segmented body (pseudometamerism)",
      "Parts": [
        {
          "Part": "Head (Scolex)",
          "Features": [
            "Apical part of Scolex is Rostellum",
            "Rostellum bears two circles of chitinous hooks (28-32)",
            "Four suckers or Acetabula",
            "Suckers and hooks are for attachment with the host body"
          ]
        },
        {
          "Part": "Neck",
          "Features": [
            "Unsegmented part",
            "New segments are formed in this region (Budding zone or Proliferation zone)",
            "Narrowest part of the body"
          ]
        },
        {
          "Part": "Strobila",
          "Features": [
            "Main body made of 800-1000 similar rectangular segments called Proglottids",
            "Youngest segments lie near the neck, oldest lie at the rear end",
            "Three types of proglottids: Young (Male organs), Mature (Male and Female organs), Gravid (Branched uterus with fertilized eggs)",
            "Gravid proglottids detach one by one and pass out with host faeces (Apolysis)"
          ]
        }
      ],
      "Body Walls": {
        "Lacks": "Cellular epidermis",
        "Consists of": [
          "Cuticle (Parasitic adaptation)",
          "Musculature",
          "Parenchyma (Mesodermal tissue)",
          {
            "Outer Layer": "Cortical parenchyma with Lime secreting gland cells",
            "Inner Layer": "Medullary parenchyma with internal organs"
          }
        ],
        "Body Cavity": "Absent (Acoelomate)"
      }
    },
    "Nutrition": {
      "Type": "Osmotrophy",
      "Details": [
        "No digestive system",
        "Depends on predigested food absorbed from host's gut through the cuticle",
        "Cuticle protects from host's digestive enzymes"
      ]
    },
    "Respiration": {
      "Type": "Anaerobic"
    },
    "Excretion": {
      "System": "Longitudinal and cross-connecting excretory canals",
      "Products": [
        "Ammonia",
        "Fatty acids"
      ],
      "Excretory Organs": "Flame cells"
    },
    "Reproduction": {
      "Type": "Hermaphrodite",
      "Details": [
        "A complete reproductive system occurs in each mature proglottid",
        "Fertilization can occur within the same proglottid or between proglottids of the same strobila",
        "Larval stages: Oncosphere, Hexacanth (with six hooks), Cysticercus (Bladder-worm)"
      ]
    },
    "Life Cycle": [
      "Each fertilized egg develops into Oncosphere larva within the proglottid",
      "Oncospheres pass out through host faeces",
      "Secondary host (Pig) ingests oncospheres",
      "Hexacanth is released in pig's stomach, travels through blood, and encysts in muscles as Bladderworm (Cysticercus)",
      "Humans get infected by eating raw or poorly cooked 'Measly pork' (infected pork)",
      "Cysticercus in pig muscles can remain viable for several years",
      "Cysticercus develops into an adult tapeworm in human intestine"
    ]
  },
  "Taenia saginata": {
    "Common Name": "Beef tapeworm",
    "Host Details": [
      "Primary host: Man",
      "Secondary host: Cattle"
    ],
    "Differences": [
      "Called 'Unarmed tapeworm' because the scolex does not possess hooks",
      "More prevalent and longer than Taenia solium"
